          <argument>
            <kind>1</kind>
            <string>s_cam_move(mouse_x, mouse_y); // mouse right
s_cam_zoom(); // mouse wheel

var accion = mouse_check_button_pressed(mb_left);
if keyboard_check_pressed(vk_anykey) {
    switch keyboard_key {
        case vk_space:
            accion = true;
            break;
        case vk_home:
            s_avance_time(true);
            break;
        case vk_end:
            s_avance_time(false);
            break;
        case vk_up:
            seleccion = 0;
            switch herramienta {
                case o_natural1:
                    seleccion = 4;
                    herramienta = o_decorado1;
                    break;
                case o_decorado1:
                    seleccion = m_grupo_indoafro;
                    herramienta = o_centro1;
                    break;
                case o_centro1:
                    seleccion = m_grupo_indoafro;
                    herramienta = o_casa1;
                    break;
                case o_casa1:
                    herramienta = o_natural1;
                    break;
            }
            break;
        case vk_down:
            seleccion = 0;
            switch herramienta {
                case o_natural1:
                    seleccion = m_grupo_indoafro;
                    herramienta = o_casa1;
                    break;
                case o_decorado1:
                    herramienta = o_natural1;
                    break;
                case o_centro1:
                    seleccion = 4;
                    herramienta = o_decorado1;
                    break;
                case o_casa1:
                    seleccion = m_grupo_indoafro;
                    herramienta = o_centro1;
                    break;
            }
            break;
        case vk_left:
            seleccion--;
            switch herramienta {
                case o_natural1:
                    if seleccion &lt; 0 {
                        seleccion = 3;
                    }
                    break;
                case o_decorado1:
                    if seleccion &lt; 4 {
                        seleccion = 10;
                    }
                    break;
                case o_centro1:
                case o_casa1:
                    if seleccion &lt; m_grupo_indoafro {
                        seleccion = m_grupo_global;
                    }
                    break;
            }
            break;
        case vk_right:
            seleccion++;
            switch herramienta {
                case o_natural1:
                    if seleccion &gt; 3 {
                        seleccion = 0;
                    }
                    break;
                case o_decorado1:
                    if seleccion &gt; 10 {
                        seleccion = 4;
                    }
                    break;
                case o_centro1:
                case o_casa1:
                    if seleccion &gt; m_grupo_global {
                        seleccion = m_grupo_indoafro;
                    }
                    break;
            }
            break;
    }
}

if accion {
    if keyboard_check(vk_shift) {
        with o_imagen {
            if visible {
                if collision_point(mouse_x, mouse_y, id, true, false) {
                    instance_destroy();
                }
            }
        }
    }
    else {
        switch herramienta {
            case o_natural1:
                s_new_natural(g_tiempo, mouse_x, mouse_y, true, seleccion);
                break;
            case o_decorado1:
                s_new_natural(g_tiempo, mouse_x, mouse_y, false, seleccion);
                break;
            case o_centro1:
                s_new_edificio(g_tiempo, mouse_x, mouse_y, true, seleccion);
                break;
            case o_casa1:
                s_new_edificio(g_tiempo, mouse_x, mouse_y, false, seleccion);
                break;
        }
    }
}

if keyboard_check_pressed(vk_delete) {
    if keyboard_check(vk_shift) {
        var otr = collision_point(mouse_x, mouse_y, o_camino, true, false);
        if otr != noone {
            with otr {
                instance_destroy();
            }
        }
    }
    else {
        instance_create(mouse_x, mouse_y, o_camino);
    }
}

</string>
