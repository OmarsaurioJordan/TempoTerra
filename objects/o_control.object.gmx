    for (var i = m_time_inicial; i &lt;= m_time_final; i++) {
        with g_parent[i, m_par_casa] {
            s_new_humano(i,
                x + random_range(-1, 1),
                y + random_range(-1, 1),
                m_genero_fem, grupo);
            s_new_humano(i,
                x + random_range(-1, 1),
                y + random_range(-1, 1),
                m_genero_masc, grupo);
        }
    }
    // calcula caminos
    with o_camino {
        rumbo = s_calcula_rejilla(x, y);
    }
}

